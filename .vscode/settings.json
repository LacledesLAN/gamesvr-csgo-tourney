{
    "cSpell.words": [
        "armsrace",
        "autokick",
        "cbble",
        "csgo",
        "cvars",
        "dockerhub",
        "gamedata",
        "gamesvr",
        "gungame",
        "laclede's",
        "lacledeâ€™s",
        "lacledeslan",
        "mapgroups",
        "maplist",
        "matchid",
        "maxrounds",
        "metamod",
        "poolday",
        "sourcemod",
        "vdfs",
        "warmod"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "markdownlint.ignore": [
        ".gitkeep",
        "LICENSE",
        "dist/get5/*.md"
    ],
    "shellcheck.exclude": ["1091"],
    "terminal.integrated.scrollback": 3500,
    "[markdown]": {
        "cSpell.fixSpellingWithRenameProvider": true,
        "cSpell.advanced.feature.useReferenceProviderWithRename": true,
        "cSpell.advanced.feature.useReferenceProviderRemove": "/^#+\\s/",
        "editor.rulers": [120],
        "editor.unicodeHighlight.ambiguousCharacters": true,
        "editor.unicodeHighlight.invisibleCharacters": true,
        "editor.wordWrap": "on"
    },
    "[shellscript]": {
        "editor.rulers": [100],
        "files.eol": "\n"
    }
}
